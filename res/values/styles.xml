<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="BackButtonStyle" parent="@android:style/Widget.Button">
        <item name="android:src">@drawable/back_button</item>
        <item name="android:background">#00000000</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_height">65dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">20dp</item>
    </style>
    <style name="SidebarButtonStyle" parent="@android:style/Widget.Button">
        <item name="android:src">@drawable/side_bar_btn1</item>
        <item name="android:background">#00000000</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_height">65dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">20dp</item>
    </style>
    <style name="SideBarLayoutDarck">
        <item name="android:layout_height">65dp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/search_bg</item>
        <item name="android:gravity">center_vertical</item>

    </style>
    <style name="SideBarLayoutSoft">
        <item name="android:layout_height">65dp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/soft_bg_side_bar</item>
    </style>
    <style name="StandartToView">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textColor" >#ffffff</item>
    </style>


    <style name="SharePanelTwitter">
        <item name="android:layout_weight">189</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:background">#00000000</item>
        <item name="android:src">@drawable/tvitter</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
    </style>
    <style name="SharePanelFacebook">
        <item name="android:layout_weight">180</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:background">#00000000</item>
        <item name="android:src">@drawable/facebook</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:layout_toRightOf">@id/share_panel_twitter</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>
    <style name="SharePanelLinkedIn">
        <item name="android:layout_weight">170</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:background">#00000000</item>
        <item name="android:src">@drawable/linkedin</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:layout_toRightOf">@id/share_panel_facebook</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>
    <style name="SharePanelMail">
        <item name="android:layout_weight">55</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:background">#00000000</item>
        <item name="android:src">@drawable/button_mail</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:layout_toRightOf">@id/share_panel_linkedin</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>

    <style name="AB_BackButton">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">53dp</item>
        <item name="android:background">#00000000</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:src">@drawable/back_button</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:layout_marginRight">3dp</item>
        <item name="android:layout_marginLeft">5dp</item>
    </style>

    <style name="AB_SideBarButton">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">38dp</item>
        <item name="android:background">#00000000</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:src">@drawable/side_bar_btn1</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:layout_marginRight">12dp</item>
        <item name="android:layout_marginLeft">8dp</item>
    </style>

    <style name="AB_FirmSymbol">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">23dp</item>
        <item name="android:background">#00000000</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:src">@drawable/logo1</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:layout_marginRight">3dp</item>
    </style>

    <style name="AB_FirmTitle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:text">@string/head_app</item>
    </style>

    <style name="AB_Background">
        <item name="android:layout_height">44dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:src">@drawable/settings_menu_bar</item>
        <item name="android:adjustViewBounds">true</item>
    </style>

    <style name="AB">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
    </style>

</resources>